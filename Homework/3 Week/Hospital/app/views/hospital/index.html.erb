<h1>Hospital Database!</h1>

<%= link_to "Patients", new_patient_path %>
<br>
<% @patient.each do |patient| %>
	<%= patient.first_name %>
	<%= patient.last_name %>
	<%= patient.DateOB %>
	<%= patient.gender %>
	<%= patient.description %>
	
	<%= link_to "Edit", edit_patient_path(patient) %>
	<%= link_to "Delete", patient_path(patient), method: :delete %>
<br>
<% end %>